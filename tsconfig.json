{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "declarationMap": false,
    "declaration": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "module": "commonjs",
    "outDir": ".",
    "lib": [
      "es2019"
    ],
    "target": "es2019",
    "isolatedModules": false,
    "noEmit": false
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
